{
    "name": "justijndepover/intercept-emails",
    "description": "Intercept outgoing emails in your Laravel application",
    "license": "MIT",
    "authors": [
        {
            "name": "Justijn Depover",
            "email": "justijndepover@gmail.com"
        }
    ],
    "require": {},
    "require-dev": {
        "orchestra/testbench": "^6.2"
    },
    "autoload": {
